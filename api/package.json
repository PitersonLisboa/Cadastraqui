{
  "name": "cadastraqui-backend",
  "version": "2.0.0",
  "description": "API do Sistema Cadastraqui - Gestão de Certificação CEBAS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "prisma db push && node dist/server.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma db push --force-reset && tsx prisma/seed.ts",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "keywords": [
    "cadastraqui",
    "cebas",
    "fastify",
    "prisma"
  ],
  "author": "RW Engenharia",
  "license": "MIT",
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/cors": "^9.0.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/jwt": "^8.0.1",
    "@fastify/multipart": "^8.1.0",
    "@fastify/static": "^7.0.1",
    "@prisma/client": "^5.10.0",
    "aws-sdk": "^2.1500.0",
    "bcryptjs": "^2.4.3",
    "date-fns": "^3.3.1",
    "dotenv": "^16.4.5",
    "exceljs": "^4.4.0",
    "fastify": "^4.26.1",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.10",
    "pdfkit": "^0.15.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.17",
    "@types/nodemailer": "^6.4.14",
    "@types/pdfkit": "^0.13.4",
    "@typescript-eslint/eslint-plugin": "^7.0.1",
    "@typescript-eslint/parser": "^7.0.1",
    "eslint": "^8.56.0",
    "prisma": "^5.22.0",
    "tsc-alias": "^1.8.8",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.0"
  }
}
